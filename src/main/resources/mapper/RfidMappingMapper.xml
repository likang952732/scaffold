<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wwinfo.mapper.RfidMappingMapper">

    <select id="getByPrintNos" resultType="com.wwinfo.model.RfidMapping" parameterType="list">
        select rfid_print_no, rfid_real_no, status from rfid_mapping
        where rfid_print_no in
        <foreach collection="list" item="printNo" index="index" open="(" close=")" separator=",">
            #{printNo}
        </foreach>
    </select>

    <update id="updateBatch">
        update  rfid_mapping set status = #{status}
        where rfid_print_no in
        <foreach collection="list" item="item" index="index" open="(" close=")" separator=",">
            #{item}
        </foreach>

    </update>
</mapper>
